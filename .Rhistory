library(rdviz)
generate_data <- function(n=500, tau = 1, beta = 0, threshold = 0,
min.X = -1, max.X = 1, sd.epsilon = 1){
X <- runif(n, min.X, max.X)
Treated <- as.numeric(X > threshold)
epsilon <- rnorm(n, 0, sd.epsilon)
Y <- tau * Treated + beta * X + epsilon
dat <- data.frame(Y,X,Treated,epsilon)
return(dat)
}
dat <- generate_data(n  = 100, tau = 0.25, beta = 0.4, sd.epsilon = 0.5)
rdviz(x=dat$X, y=dat$Y)
